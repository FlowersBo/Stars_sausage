/* pages/mine/integral/cutDetail/cutDetail.wxss */
.integral {
  position: absolute;
  width: 690rpx;
  height: 200rpx;
  background: #fff;
  top: 200rpx;
  left: 50%;
  margin-left: -345rpx;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0 50rpx;
}
.formClass{
  width: 690rpx;
  height: 600rpx;
  position: absolute;
  top: 460rpx;
  left: 50%;
  margin-left: -345rpx;
  box-sizing: border-box;
  padding: 40rpx 60rpx;
  background: #fff;
  border-radius: 20rpx;
  box-shadow: 2rpx 10rpx 12rpx rgba(0, 0, 0, .1);
  font-size: 28rpx;
}

.formClass .inputWrap{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 52rpx;
}
.jifen{
  width: 100%;
  height: 80rpx;
  font-size: 24rpx;
  color: #aaa;
  text-align: right;
}
.inputWrap input{
  width: 65%;
  height: 80rpx;
  border: 2rpx solid #ddd;
  border-radius: 16rpx;
  box-sizing: border-box;
  padding-left: 16rpx;
}
.formBtn{
  width: 100%;
  height: 100rpx;
  font-size: 40rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  background: #FF911D;
  border-radius: 50rpx;
  margin-top: 100rpx;
}